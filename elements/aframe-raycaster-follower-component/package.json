{
  "name": "@odl/aframe-raycaster-follower-component",
  "version": "1.0.11-alpha.0",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EberlyODL/vr/tree/master/elements/aframe-raycaster-follower-component"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "budo examples/main.js:build.js --dir examples --port 8000 --live --open",
    "build": "npm run build:example && npm run build:dist",
    "build:example": "browserify examples/main.js -o examples/build.js",
    "build:dist": "webpack index.js dist/aframe-raycaster-follower-component.js && webpack -p index.js dist/aframe-raycaster-follower-component.min.js",
    "lint": "semistandard -v | snazzy",
    "preghpages": "npm run build && shx rm -rf gh-pages && shx mkdir gh-pages && shx cp -r examples/* gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr"
  ],
  "dependencies": {
    "aframe": "^0.8.2"
  },
  "devDependencies": {
    "aframe-animation-component": "^5.1.2",
    "browserify": "^12.0.1",
    "browserify-css": "^0.8.3",
    "budo": "^7.1.0",
    "ghpages": "0.0.3",
    "inquirer": "^1.0.2",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "snazzy": "^4.0.0",
    "webpack": "^1.12.9"
  },
  "ignore": [
    "examples/build.js",
    "dist/**"
  ],
  "gitHead": "6f1bb4cf991d964b475e6baf9c923e727265c079",
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
